package oopsproject;
class pen{
	String color;
	String type;
	
	public void write()
	{
		System.out.println("Writing Something.."+color+type);
	}
	
	
}
class student{
	student()
	{
		System.out.println("Dev");
	}
}
public class Tesr2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        pen pen1=new pen();
        pen1.color="Red";
        pen1.type="ball";
        pen1.write();
        student s1=new student();
	}

}
